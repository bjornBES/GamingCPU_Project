	mov   HL,                      65536
	mov   [DS:B],                  HL
	; _pVram = 65536
L0000023:
	mov   R20,                     far @__DATASTRAT__ + 0
	mov   A,                       85
	mov   [R20]                    A
L0000024:
	mov   HL,                      65536
	mov   [HL]                     A
L0000025:
	mov   HL,                      MessERS
