; if
_Label0:
	mov   HL,                      [SP - 4]
	mov   R19,                     HL
	mov   HL,                      MessERS
	mov   R20,                     HL
	cmp   R19,                     R20
	jne   [_Label0_IfExit]
	mov   A,                       1
	jmp   [Exit_main]
_Label0_IfExit:
; else
	mov   A,                       2
